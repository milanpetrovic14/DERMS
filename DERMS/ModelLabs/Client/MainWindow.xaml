<Window x:Class="Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="460" Width="810">
    <Grid Margin="0,0,399.6,12">
        <TabControl HorizontalAlignment="Left" Height="398" Margin="10,10,0,0" VerticalAlignment="Top" Width="384">
            <TabItem Header="Get Values">
                <Grid Background="#FFE5E5E5" Margin="0,1,-2.6,-1.6">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="47*"/>
                        <ColumnDefinition Width="144*"/>
                    </Grid.ColumnDefinitions>
                    <!--<Grid.ColumnDefinitions>
                        <ColumnDefinition Width="39*"/>
                        <ColumnDefinition Width="215*"/>
                    </Grid.ColumnDefinitions>-->
                    <ComboBox Name="cb1" HorizontalAlignment="Left" Margin="10,32,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="ComboBox_SelectionChanged" Grid.ColumnSpan="2"/>
                    <Label Content="GID" HorizontalAlignment="Left" Margin="10,6,0,0" VerticalAlignment="Top"/>
                    <ComboBox Name="cb12" HorizontalAlignment="Left" Margin="10,121,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="Cb12_SelectionChanged" Grid.ColumnSpan="2"/>
                    <Label Content="Property" HorizontalAlignment="Left" Margin="10,90,0,0" VerticalAlignment="Top"/>
                    <Label Content="Izabrani property" HorizontalAlignment="Left" Margin="68.5,5,0,0" VerticalAlignment="Top" Grid.Column="1"/>
                    <ListBox Name="lb1" HorizontalAlignment="Left" Height="116" Margin="68.5,34,0,0" VerticalAlignment="Top" Width="183" Grid.Column="1"/>
                    <Button Content="GetValues" HorizontalAlignment="Left" Margin="10,216,0,0" VerticalAlignment="Top" Width="120" Click="Button_Click" Height="22" Grid.ColumnSpan="2"/>
                    <TextBlock Name="tb1" IsEnabled="False" Background="White" HorizontalAlignment="Left" Margin="68.5,216,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="183" Height="22" Grid.Column="1"/>
